
CatalinaGlobalRequestProcessor:
  - mbean:    Catalina:type=GlobalRequestProcessor,*
    attr:     bytesReceived
    rate:     60
    label:    Catalina.GlobalRequestProcessor.$Name.bytesReceived

  - mbean:    Catalina:type=GlobalRequestProcessor,*
    attr:     bytesSent
    rate:     60
    label:    Catalina.GlobalRequestProcessor.$Name.bytesSent

  - mbean:    Catalina:type=GlobalRequestProcessor,*
    attr:     errorCount
    rate:     60
    label:    Catalina.GlobalRequestProcessor.$Name.errorCount

  - mbean:    Catalina:type=GlobalRequestProcessor,*
    attr:     processingTime
    rate:     60
    label:    Catalina.GlobalRequestProcessor.$Name.processingTime

  - mbean:    Catalina:type=GlobalRequestProcessor,*
    attr:     requestCount
    rate:     60
    label:    Catalina.GlobalRequestProcessor.$Name.requestCount


CatalinaThreadPool:
  - mbean:    Catalina:type=ThreadPool,*
    attr:     currentThreadCount
    rate:     60
    label:    Catalina.ThreadPool.$Name.currentThreadCount

  - mbean:    Catalina:type=ThreadPool,*
    attr:     maxThreads
    rate:     300
    label:    Catalina.ThreadPool.$Name.maxThreads